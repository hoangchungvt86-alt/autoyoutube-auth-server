{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "FIREBASE_CREDENTIALS": "@firebase_credentials",
    "SECRET_KEY": "@secret_key",
    "ADMIN_KEY": "@admin_key"
  }
}
